{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
          "SingleLine": true,
          "IncludeScopes": true,
          "TimestampFormat": "HH:mm:ss ",
          "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
